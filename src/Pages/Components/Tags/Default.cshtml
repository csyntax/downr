@model string[]

    <li class="nav-item dropdown">
        <a href="#"
           class="nav-link dropdown-toggle"
           id="navbarDropdown"
           role="button"
           data-toggle="dropdown"
           aria-haspopup="true"
           aria-expanded="false">
            Categories
        </a>
        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
            @foreach (var tag in Model)
            {
                <a class="dropdown-item" asp-page="./Category" asp-route-name="@tag">
                    @tag
                </a>
            }
        </div>
    </li>