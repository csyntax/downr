@page
@model IndexModel
@{
    ViewData["Title"] = "Home";
}

<ul>
    @foreach(var post in Model.Posts)
    {
        <li>
            <a asp-page="./Post" asp-route-id="@post.Slug">
                @post.Title
            </a>
        </li>
    }
</ul>
<hr />
<ul>
    @foreach (var tag in Model.Tags)
    {
        <li>@tag</li>
    }
</ul>